#tag WindowCode
	#tag Method, Flags = &h1000
		Sub Constructor(ftc as FormattedText)
		  // Calling the overridden superclass constructor.
		  Super.Constructor
		  
		  ftc1 = ftc
		  
		  Load
		  
		  self.ShowModal
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Load()
		  dim b as boolean
		  
		  dim cHyperlink as color
		  dim cRollover as color
		  dim cVisited as color
		  dim cDisabled as color
		  dim bHyperlink as Boolean
		  dim bRollover as boolean
		  dim bVisited as boolean
		  dim bDisabled as boolean
		  
		  
		  b = ftc1.selectionHyperLinkColor(cHyperlink, cRollover, cVisited, cDisabled, bHyperlink, bRollover, bVisited, bDisabled)
		  
		  txthyperlink.text = FTUtilities.colorToString(cHyperlink)
		  txtRollover.text = FTUtilities.colorToString(cRollover)
		  txtVisited.text = FTUtilities.colorToString(cVisited)
		  txtDisabled.text = FTUtilities.colorToString(cDisabled)
		  chkHyperlink.value = bHyperlink
		  chkRollover.value = bRollover
		  chkVisited.value = bVisited
		  chkDisabled.value = bDisabled
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Sub Save()
		  dim cHyperlink as color
		  dim cRollover as color
		  dim cVisited as color
		  dim cDisabled as color
		  dim bHyperlink as Boolean
		  dim bRollover as boolean
		  dim bVisited as boolean
		  dim bDisabled as boolean
		  
		  cHyperlink = FTUtilities.stringToColor( txthyperlink.text )
		  cRollover = FTUtilities.stringToColor( txtRollover.text)
		  cVisited = FTUtilities.stringToColor( txtVisited.text)
		  cDisabled = FTUtilities.stringToColor( txtDisabled.text)
		  
		  
		  bHyperlink = chkHyperlink.value
		  bRollover = chkRollover.value
		  bVisited = chkVisited.value
		  bDisabled = chkDisabled.value
		  
		  ftc1.changeHyperLinkColor(cHyperlink, cRollover, cVisited, cDisabled, bHyperlink, bRollover, bVisited, bDisabled)
		  
		  self.close
		  
		End Sub
	#tag EndMethod


	#tag Property, Flags = &h0
		ftc1 As FormattedText
	#tag EndProperty


#tag EndWindowCode

#tag Events pbSave
	#tag Event
		Sub Action()
		  save
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events pbCancel
	#tag Event
		Sub Action()
		  self.close
		End Sub
	#tag EndEvent
#tag EndEvents
